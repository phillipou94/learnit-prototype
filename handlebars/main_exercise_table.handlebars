<div class="table-header">
    <h1> Exercises </h1>
    <div class="progress-container">
        {{> main_progress_container progress=progress}}
    </div>
    <div class="dropdown">
        <span id="sort-title">{{sort_display }}</span>
        <img src="./assets/DropdownIndicator.svg" />
        <div class="dropdown-content">
            <p class="dropdown-option" id ="option1">Due Date</p>
            <p class="dropdown-option" id ="option2">Incomplete</p>
            <p class="dropdown-option" id ="option3">Completed</p>
        </div>
    </div>
</div>


{{#each exercises}}
    {{> main_exercise_row exercise=this}}
{{/each}}
